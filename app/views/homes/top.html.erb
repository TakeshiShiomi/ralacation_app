<h3>home</h3>

<div>
  <%= link_to 'logout', destroy_user_session_path, method: :delete %>
</div>

<div>
  <%= link_to '投稿', [:new, :photo] %>
</div>

<% @feeds.each do |photo| %>
  <div>
    <div>
      <%= link_to photo.user.email, [photo.user] %>
    </div>
    <div>
      <% if photo.image.attached? %>
        <%= image_tag photo.image %>
      <% else %>
        <%= image_tag("https://placehold.jp/300x300.png") %>
      <% end %>
    </div>
    <div>
      <%= photo.address %>
    </div>
    <div>
      <%= photo.caption %>
    </div>
<% end %>
